		script.Parent.TextColor3 = Color3.fromRGB(140, 0, 255)
		script.Parent.Parent.TabButton.TextColor3 = Color3.fromRGB(255,255,255)
		wait(0.1)
		script.Parent.Parent.Parent.MainTab.Visible = true
		script.Parent.Parent.Parent.VisTab.Visible = false

		--The Pfp and Name
		wait(0.1)

		--Other
	end)
end
